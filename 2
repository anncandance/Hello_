
#include <stdio.h>
#include <stdlib.h>
#include <locale.h>
#include <math.h>

int main() {
	setlocale(LC_ALL, "Russian");
	int x, i=0, c, m=0, c1, c2;
	scanf("%d", &x);
	for(; pow(2, i) < x; i++) {c = (x>>i)&1; if(c==1) {i++; c1= (x>>i)&1; i++; c2 = (x>>i)&1}; \
	if(c1 == 0 && c2 == 1) m += 1};
	
	
	return 0;
	
	
	
	
	
}
